  "devDependencies": {
    "@babel/preset-env": "^7.24.7",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@vitest/coverage-istanbul": "^0.34.6",
    "codecov": "^3.8.3",
    "nuxt": "^3.11.2",
    "prettier": "^3.3.2",
    "shipjs": "0.26.3",
    "tsup": "^8.1.0",
    "typescript": "^5.5.4",
    "vitest": "^0.34.6"
  },
  "keywords": [
    "attribute",
    "head",
    "meta",
    "seo",
    "vue",
    "nuxt",
    "jsonld"
